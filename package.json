{
  "name": "nexus-tutorial",
  "version": "0.1.0",
  "description": "Tutorial example using Nexus GraphQL, Apollo Server and Prisma",
  "main": "index.js",
  "scripts": {
    "dev": "dotenv -e .env.dev ts-node-dev --transpile-only --no-notify api/index.ts",
    "build": "tsc",
    "generate": "ts-node --transpile-only api/schema",
    "test": "dotenv -e .env.test jest",
    "pretest": "dotenv -e .env.test npm run generate "
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^2.27.0",
    "apollo-server": "^3.0.2",
    "apollo-server-express": "^3.0.2",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "nexus": "^1.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "dotenv-cli": "^4.0.0",
    "get-port": "^5.1.1",
    "graphql-request": "^3.4.0",
    "jest": "^27.0.6",
    "jest-extended": "^0.11.5",
    "prisma": "^2.27.0",
    "ts-jest": "^27.0.4",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  }
}
